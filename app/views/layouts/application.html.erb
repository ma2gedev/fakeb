<!DOCTYPE html>
<html>
<head>
  <title>Fakeb</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="flash">
    <% if flash[:notice].present? %>
      <p class="flash_notice"><%= flash[:notice] %></p>
    <% end %>
    <% if flash[:alert].present? %>
      <p class="flash_alert"><%= flash[:alert] %></p>
    <% end %>
  </div>

<%= yield %>

</body>
</html>
