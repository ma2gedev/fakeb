<div class="page-header">
  <h1><%= @user_name %>のブックマーク</h1>
</div> 
<% @bookmarks.each do |bookmark| %>
<div class="row-fluid show-grid">
  <div class="span9">
    <h3><%= link_to bookmark.entry.title, bookmark.entry.url, :target => "_blank" %></h3>
    <p><%= bookmark.comment %></p>
  </div>
</div>
<% end %>
